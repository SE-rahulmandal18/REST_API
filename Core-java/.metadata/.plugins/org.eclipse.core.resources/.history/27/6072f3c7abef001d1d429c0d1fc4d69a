package demo;

public class Geek {

	public static void main(String[] args) {
		
		//Father f = new Father();
		
		Son s = new Son();
		Daughter d = new Daughter();
		s.disp();
		d.disp();
	}
}

abstract class Father {
	
	 void disp1() {
		 System.out.println("this is java program");
	 }
}

class Son extends Father{
	void disp() {
		System.out.println("Abstract method is defined in subsclass son");
	}
}
class Daughter extends Father{
	void disp() {
		System.out.println("Abastract method is defined in subclass daughter");
	}
}