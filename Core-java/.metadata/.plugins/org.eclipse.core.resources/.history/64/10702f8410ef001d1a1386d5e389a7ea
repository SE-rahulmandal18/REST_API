public class AccessModifierMain {

	public static void main(String[] args) {
		
   Apple apple1 = new Apple();
   
   //System.out.println("Apple's price= "+ apple1.price)); //compiler error for access permission
   
   //System.out.println("Apple's weigth= "+ apple1.weight); //compiler error for access permission
	

   System.out.println("Inside com.simpli.util package Apple's weigth= "+ apple1.color); // WORKS

   SnowApple snowapple1 = new SnowApple();

 System.out.println("Apple's city of origin = "+ snowapple1.displayCityOfOrigin()); // WORKS

	}

}
